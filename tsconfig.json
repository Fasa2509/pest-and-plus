{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "noImplicitAny": false,
    // "baseUrl": "C:/Users/Miguel/Desktop/Proyectos/work-learning/src",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@customTypes/*": [
        "./src/types/*"
      ],
      "@errors/*": [
        "./src/errors/*"
      ],
      "@database/*": [
        "./src/database/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}